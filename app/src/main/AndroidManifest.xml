        android:theme="@style/AppTheme">
        <activity android:name=".AuthenticationActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".Coats.CoatsSpecActivity" />
        <activity android:name=".Coats.GenerateCoatsActivity" />
        <activity android:name=".PDFActivity" />
        <activity android:name=".Coveralls.CoverallsSpecActivity" />
        <activity android:name=".Coveralls.GenerateCoverallActivity" />
        <activity android:name=".Jackets.JacketsSpecActivity" />
        <activity android:name=".Jackets.GenerateJacketActivity" />
        <activity android:name=".Trousers.GenerateTrouserActivity" />
        <activity android:name=".Trousers.TrousersSpecActivity"></activity>
    </application>

</manifest>